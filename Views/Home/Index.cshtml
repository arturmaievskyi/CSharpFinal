
@RenderBody{

    <main>
    @foreach (var p in Model)
    {
        <div class="card" style="width: 18rem;">
            <img src="@p.image" class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">@p.Name</h5>
                <p class="card-text">@p.price</p>
                <button class="order"></button>
            </div>
        </div>
    }
    <div class=".cart">
    
    </div>
</main>


}
